<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#ff35bfef" />
            <stroke
                android:width="1dp"
                android:color="#ff062999" />
            <corners
                android:radius="58dp" />

        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="@color/default_dialog_button_bg_pressed"
                android:endColor="@color/default_dialog_button_bg"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="@color/default_dialog_button_bg" />
            <corners
                android:radius="58dp" />

        </shape>
    </item>
</selector>