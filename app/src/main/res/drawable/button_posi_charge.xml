<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="@color/posi_button_charge_pressed" />
            <stroke
                android:width="1dp"
                android:color="@color/posi_button_border_charge" />
            <corners
                android:radius="58dp" />

        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="@color/posi_button_charge" />

            <stroke
                android:width="1dp"
                android:color="@color/posi_button_border_charge" />
            <corners
                android:radius="58dp" />

        </shape>
    </item>
</selector>